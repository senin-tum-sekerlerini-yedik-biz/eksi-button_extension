{
  "manifest_version": 3,
  "name": "eksi toolbar",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "images/icon32.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["contextMenus", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  }
}
